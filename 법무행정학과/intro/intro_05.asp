	jQuery( 'td[rowspan]', table ).each( function() {
		var rows_ran = parseInt( jQuery( this ).attr( 'rowspan' ), 10 );
		if( rows_ran >= trlen ){
			jQuery( this ).addClass( 'rows_01' );
		}else{
			jQuery( this ).addClass( 'rows_02' );
			if( parseInt( jQuery( this ).text(), 10 ) != 2005 ){
				jQuery( this ).css( 'border-bottom', '1px solid #6b6b6b' );
				var paNext = jQuery( this ).parent().nextUntil( 'tr:has(td[rowspan])' );
				jQuery( 'td', paNext[ paNext.length-1 ] ).css( 'border-bottom', '1px solid #6b6b6b' );
			}
		}
	} );

	jQuery( 'tr', table ).each( function() {
		jQuery( 'td:last', this ).addClass( 'last_td' );
	} );
} );
//]]>
</script>
